/* Basic dashboard styles */
body {
    font-family: Arial, sans-serif;
    background-color: #f5f5f5;
    margin: 0;
    padding: 0;
    /*background-image: url('../images/nairobi.jpg'); no-repeat center;*/
    background-color: #FFCD4B;
    background-repeat: no-repeat;
    background-size: cover;
}
header {
    background-color: #333;
    color: #fff;
    padding: 10px 0;
}
nav ul {
    list-style-type: none;
    padding: 0;
    margin: 0;
}

nav ul li {
    display: inline;
    margin-right: 10px;
}

nav ul li a {
    text-decoration: none;
    color: #fff;
}

main {
    max-width: 800px;
    margin: 20px auto;
    background-color: #fff;
    padding: 20px;
    border-radius: 5px;
    box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.2);
}
.user-profile h1 {
    font-size: 24px;
    margin-bottom: 10px;
}
.order-history ul {
    list-style-type: none;
    padding: 0;
}

.order-history ul li {
    margin-bottom: 10px;
}

